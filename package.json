{
    "name": "aliaksei-hurynovich-cv",
    "version": "0.1.1",
    "private": true,
    "description": "Aliaksei Hurynovich's CV app",
    "license": "MIT",
    "author": {
        "name": "Aliaksei Hurynovich",
        "email": "shagon1k@gmail.com",
        "url": "https://ahurynovich.com/"
    },
    "engines": {
        "node": "14.x",
        "npm": "8.x"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development WITH_SSR=false webpack serve --config config/webpack/client/webpack.config.client.babel.js",
        "build:client": "cross-env WITH_SSR=false webpack --config config/webpack/client/webpack.config.client.babel.js",
        "build:client:dev": "cross-env NODE_ENV=development npm run build:client",
        "build:client:prod": "cross-env NODE_ENV=production npm run build:client",
        "build:client:wssr": "cross-env WITH_SSR=true webpack --config config/webpack/client/webpack.config.client.babel.js",
        "build:client:wssr:dev": "cross-env NODE_ENV=development npm run build:client:wssr",
        "build:client:wssr:prod": "cross-env NODE_ENV=production npm run build:client:wssr",
        "build:server": "cross-env WITH_SSR=true webpack --config config/webpack/server/webpack.config.server.babel.js",
        "build:server:dev": "cross-env NODE_ENV=development npm run build:server",
        "build:server:prod": "cross-env NODE_ENV=production npm run build:server",
        "build:app:wssr:dev": "npm run build:client:wssr:dev && npm run build:server:dev",
        "build:app:wssr:prod": "npm run build:client:wssr:prod && npm run build:server:prod",
        "start": "npm run dev",
        "start:wssr": "node dist/server",
        "start:wssr:debug": "nodemon --inspect dist/server",
        "rimraf": "./node_modules/rimraf/bin.js",
        "build:clean": "rimraf ./dist",
        "test": "jest --config config/jest/jest.config.js --notify",
        "lint": "npm run lint:scripts && npm run lint:styles",
        "lint:fix": "npm run lint:scripts:fix && npm run lint:styles:fix",
        "lint:scripts": "eslint ./src",
        "lint:scripts:fix": "eslint ./src --fix",
        "lint:styles": "stylelint ./src/**/**/*.scss",
        "lint:styles:fix": "stylelint ./src/**/**/*.scss --fix",
        "deploy:s3": "sls client deploy --no-config-change --no-policy-change --no-cors-change",
        "deploy:s3:nc": "npm run deploy:s3 -- --no-confirm",
        "build:and:deploy:s3": "npm run build:clean && npm run build:client:prod && npm run deploy:s3",
        "build:and:deploy:s3:nc": "npm run build:clean && npm run build:client:prod && npm run deploy:s3:nc",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": "./config/lint/eslint/eslint.config.js",
        "ignorePatterns": [
            "__snapshots__"
        ]
    },
    "stylelint": {
        "extends": "./config/lint/stylelint/stylelint.config.js",
        "ignoreFiles": [
            "src/**/**/reset.scss"
        ]
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix"
        ],
        "*.(s)css": [
            "stylelint --fix"
        ]
    },
    "dependencies": {
        "fs-extra": "^9.0.1",
        "helmet": "^4.6.0",
        "i18next": "^21.6.3",
        "mobile-detect": "^1.4.5",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.1.0",
        "react-helmet-async": "^1.2.2",
        "react-redux": "^7.2.1",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "^7.12.1",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-regenerator": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.12.1",
        "@babel/register": "^7.12.1",
        "@webpack-cli/serve": "^1.0.1",
        "autoprefixer": "^10.3.7",
        "autoprefixer-core": "^6.0.1",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.5.2",
        "babel-loader": "^8.1.0",
        "cross-env": "^7.0.2",
        "css-loader": "^5.2.7",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "enzyme-to-json": "^3.6.1",
        "eslint": "^7.11.0",
        "eslint-config": "^0.3.0",
        "eslint-config-prettier": "^6.13.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.21.4",
        "html-webpack-plugin": "^4.5.0",
        "husky": "^7.0.4",
        "incstr": "^1.2.3",
        "jest": "^26.5.3",
        "jest-environment-enzyme": "^7.1.2",
        "jest-enzyme": "^7.1.2",
        "lint-staged": "^12.1.2",
        "mini-css-extract-plugin": "^1.6.2",
        "nodemon": "^2.0.15",
        "postcss-loader": "^4.3.0",
        "prettier": "^2.1.2",
        "rimraf": "^3.0.2",
        "sass": "^1.43.2",
        "sass-loader": "^10.2.0",
        "serverless": "^2.8.0",
        "serverless-finch": "^2.6.0",
        "style-loader": "^2.0.0",
        "stylelint": "^14.1.0",
        "stylelint-config-standard-scss": "^3.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-prettier": "^2.0.0",
        "stylelint-scss": "^4.0.1",
        "webpack": "^5.1.3",
        "webpack-cli": "^4.0.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.2.0",
        "webpackbar": "^4.0.0"
    },
    "browserslist": [
        "last 3 versions",
        ">3%",
        "not dead",
        "not op_mini all",
        "not ie < 11",
        "not safari < 11",
        "not IOS < 11"
    ]
}
